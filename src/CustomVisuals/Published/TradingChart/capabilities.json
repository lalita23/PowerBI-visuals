        {
            "conditions": [
                {
                    "date": {
                        "max": 1
                    },
                    "low": {
                        "max": 1
                    },
                    "high": {
                        "max": 1
                    },
                    "open": {
                        "max": 1
                    },
                    "close": {
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "date"
                    },
                    "dataReductionAlgorithm": {
                        "top": {}
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "low"
                            }
                        },
                        {
                            "bind": {
